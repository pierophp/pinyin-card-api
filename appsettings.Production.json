{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "mysqlconnection": {
    "connectionString": "server=${DATABASE_HOST};userid=${DATABASE_USER};password=${DATABASE_PASSWORD};database=pinyin_card;"
  },
  "cloudinary": {
    "account": "pierophp",
    "apiKey": "${CLOUDINARY_API_KEY}",
    "apiSecret": "${CLOUDINARY_API_SECRET}",
    "folder": "cards"
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Google": {
      "ClientId": "${GOOGLE_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
    }
  }
}
